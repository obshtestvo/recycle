[uwsgi]
#workers = 2
# master
master = true
# maximum number of processes
#processes = 10
# the socket (use the full path to be safe)
# with appropriate permissions - *may* be needed
socket  = /tmp/recycle.sock
# chmod-socket    = 664
chdir = /home/ubuntu/web/recycle
module = recycle.wsgi
# the virtualenv
home = /home/ubuntu/.virtualenvs/recycle
# clear environment on exit
vacuum = true